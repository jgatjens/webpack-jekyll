{
  "name": "project_name",
  "version": "1.0.0",
  "description": "project_name website",
  "main": "webpack.dev.js",
  "engines": {
    "node": ">=8.5.0"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 10"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run clean && concurrently 'npm run jekyll:watch' 'npm run webpack:watch'",
    "prod": "npm run clean && npm run webpack && npm run jekyll",
    "webpack": "webpack --config config/webpack.prod.js",
    "webpack:watch": "webpack-dev-server --config config/webpack.dev.js",
    "webpack:dev": "webpack --config config/webpack.dev.js",
    "jekyll": "JEKYLL_ENV=production jekyll build --config config/_config.prod.yml",
    "jekyll:dev": "JEKYLL_ENV=development jekyll build --config config/_config.dev.yml",
    "jekyll:watch": "nodemon -w src -e html,svg -x npm run jekyll:dev",
    "css": "node-sass --include-path ./node_modules ./scss/site.scss ./dist/site.css --source-map-embed",
    "watch-css": "nodemon -e scss  -x npm run css",
    "style-guide": "node-sass ./scss/style-guide.scss ./dist/style-guide.css",
    "server": "lite-server --baseDir='dist'"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "npm": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "autoprefixer": "^7.2.4",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "closest": "0.0.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "kss": "^3.0.0-beta.18",
    "kss-webpack-plugin": "^1.3.1",
    "lite-server": "^2.3.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.4",
    "onchange": "^3.3.0",
    "postcss-loader": "^2.1.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.2",
    "store": "^2.0.12",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^0.6.2",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jgatjens/webpack-jekyll.git"
  },
  "keywords": [
    "jekyll",
    "webpack 4",
    "bulma"
  ],
  "author": "jgatjens.com",
  "license": "ISC"
}
